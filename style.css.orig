/*Pop-up de Login*/

h1 {
  text-align: center;
  font-family: Tahoma, Arial, sans-serif;
  color: #ffea00;
  margin: 20px 0;
}

.box {
  width: 40%;
  margin: 0 auto;
  background: rgba(255,255,255,0.2);
  padding: 35px;
  border: 2px solid #fff;
  border-radius: 20px/50px;
  background-clip: padding-box;
  text-align: center;
  
}

.overlay {
  position: fixed;
  width: 100%;
  height: 100%;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  background: rgba(0, 0, 0, 0.7);
  transition: opacity 500ms;
  visibility: hidden;
  opacity: 0;
  z-index: 1002;
}

.overlay:target {
  visibility: visible;
  opacity: 1;
}

.popup , .popupcad {
  margin: 70px auto;
  padding: 20px;
  width: 300px;
  position: relative;
  background: #313131;
  border-radius: 24px;
  margin-top: 5%;
}

.popup p , .popupcad p{
  margin-top: 25px;
  font-family: Tahoma, Arial, sans-serif;
  color: #ffea00;
  text-align: center;
  text-transform: uppercase;
  font-weight: 500;
}

.popup .close , .popupcad .close{
  color: #ffea00;
  position: absolute;
  top: 20px;
  right: 30px;
  transition: all 200ms;
  font-size: 30px;
  font-weight: bold;
  text-decoration: none;
  color: white;
}

.popup .close:hover{
  color: #ffea00;
}

.popupcad .close:hover{
  color: #ffea00;
}

.popup .content , .popupcad .contentcad{
  overflow: auto;
}
   
.popup .content input[type = "text"], .popup .content input[type = "password"], .popupcad .contentcad input[type = "text"], .popupcad .contentcad input[type = "password"]{
  border:0;
  background: none;
  display: block;
  margin: 20px auto;
  text-align: center;
  border: 2px solid #ffea00;
  padding: 14px 10px;
  width: 180px;
  outline: none;
  color: white;
  border-radius: 24px;
  transition: 0.25s;
}
.popup .content input[type = "text"]:focus, .content input[type = "password"]:focus, .popupcad .contentcad input[type = "text"]:focus, .popupcad .contentcad input[type = "password"]:focus{
  width: 220px;
  border-color: #ffea00;
}

.popup .content input[type = "submit"], .popupcad .contentcad input[type = "submit"]{
  border:0;
  background: none;
  display: block;
  margin: 20px auto;
  text-align: center;
  border: 2px solid #ffea00;
  padding: 14px 40px;
  outline: none;
  color: white;
  border-radius: 24px;
  transition: 0.25s;
  cursor: pointer;
}

.popup .content input[type = "submit"]:hover, .popupcad .contentcad input[type = "submit"]:hover{
  background: #ffea00;
}

.submit{
  border:0;
  background: none;
  display: block;
  margin: 20px auto;
  text-align: center;
  border: 2px solid #ffea00;
  padding: 10px 10px;
  outline: none;
  color: white;
  border-radius: 24px;
  transition: 0.25s;
  cursor: pointer;
}

.submit:hover{
  background: #ffea00;
}

.content a{
    color: white;
    text-align: center;
    padding-left: 70px;
}


